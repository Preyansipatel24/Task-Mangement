@{
    ViewBag.dataTable = "true";
    string Title = "Project";
    string PageTitle = Title + " Assignment User List";
}
<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1>@PageTitle</h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item">@Title</li>
                    <li class="breadcrumb-item"><a href="/Project/Index">@Title List</a></li>
                    <li class="breadcrumb-item active">@PageTitle</li>
                </ol>
            </div>
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
<!-- Main content -->
<div class="content">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <input type="hidden" id="hdnProjectId" name="hdnProjectId" value="@Model" />
                <div class="card card-primary card-outline">
                    <div id="divProjectAssignmentList">
                        @* <div class="card-header">
                        <div class="row">
                        <div class="col-md-6 card_title_center">
                        <h2 class="card-title"><b>@PageTitle</b></h2>
                        </div>
                        <div class="col-md-6">
                        </div>
                        </div>
                        </div>
                        <div class="card-body">
                        <div id="divProjectAssignmentList">
                        </div> *@
                    </div>
                    <div class="card-footer">
                        <input type="button" id="btnUpdateProjectAssignment" name="btnUpdateProjectAssignment" class="btn btn-success float-right ml-1" value="Update" />
                        <input type="button" id="btnCancel" name="btnCancel" class="btn btn-danger float-right" value="Cancel" />
                    </div>
                </div>
            </div>
        </div>

    </div><!-- /.container-fluid -->
</div>
<!-- /.content -->
@section Scripts
{
    <script src="~/scripts/project_assign.js?_@DateTime.Now.Ticks"></script>
}
